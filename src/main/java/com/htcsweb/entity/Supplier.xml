<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.htcsweb.dao.SupplierDao">
    <!--<select id="getSupplierById" parameterType="int" resultType="com.zking.entity.Supplier">-->
        <!--select * from veh_tru_Supplier where supplier_id=#{supplier_id}-->
    <!--</select>-->

    <insert id="addSupplier" parameterType="com.htcsweb.entity.Supplier">
        INSERT INTO veh_tru_supplier(supplier_code,supplier_name,forwar_address,contact_phone,clearing_form,is_validity,bank,instructions,picture,mac_c_no,mac_s_no) VALUES (#{supplier_code},#{supplier_name},#{forwar_address},#{contact_phone},#{clearing_form},#{is_validity},#{bank},#{instructions},#{picture},#{mac_c_no},#{mac_s_no})
</insert>

    <select id="getSupplier" resultType="com.htcsweb.entity.Supplier">
        SELECT * from veh_tru_supplier
    </select>

    <update id="updateSupplier" parameterType="com.htcsweb.entity.Supplier">
        UPDATE veh_tru_supplier set supplier_code=#{supplier_code},supplier_name=#{supplier_name},forwar_address=#{forwar_address},contact_phone=#{contact_phone},clearing_form=#{clearing_form},is_validity=#{is_validity},bank=#{bank},instructions=#{instructions},picture=#{picture},mac_c_no=#{mac_c_no},mac_s_no=#{mac_s_no} WHERE supplier_id=#{supplier_id}
    </update>


    <select id="getcount" resultType="int">
        select count(*) from veh_tru_supplier
    </select>

    <resultMap id="all" type="com.htcsweb.entity.Supplier">
        <id property="supplier_id" column="supplier_id"></id>
        <result property="supplier_code" column="supplier_code"></result>
        <result property="supplier_name" column="supplier_name"></result>
        <result property="forwar_address" column="forwar_address"></result>
        <result property="contact_phone" column="contact_phone"></result>
        <result property="clearing_form" column="clearing_form"></result>
        <result property="is_validity" column="is_validity"></result>
        <result property="bank" column="bank"></result>
        <result property="instructions" column="instructions"></result>
        <result property="picture" column="picture"></result>
        <result property="mac_c_no" column="mac_c_no"></result>
        <result property="mac_s_no" column="mac_s_no"></result>
    </resultMap>

    <select id="feny"  resultMap="all">
        select * from veh_tru_supplier where supplier_id limit #{arg0},#{arg1}
    </select>

    <select id="getSupplierById" parameterType="int" resultType="com.htcsweb.entity.Supplier">
        select * from veh_tru_supplier where supplier_id=#{supplier_id}
    </select>
        </mapper>