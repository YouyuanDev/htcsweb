<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.htcsweb.dao.OdStencilProcessDao">


    <insert id="addOdStencilProcess" parameterType="com.htcsweb.entity.OdStencilProcess">
    INSERT INTO od_stencil_process(pipe_no,operation_time,operator_no,stencil_content,center_line_color,pipe_end_color,upload_files,remark,result) VALUES (#{pipe_no},#{operation_time},#{operator_no},#{stencil_content},#{center_line_color},#{pipe_end_color},#{upload_files},#{remark},#{result})
    </insert>

    <select id="getOdStencilProcess" resultType="com.htcsweb.entity.OdStencilProcess">
        SELECT * from od_stencil_process
    </select>

    <update id="updateOdStencilProcess" parameterType="com.htcsweb.entity.OdStencilProcess">
    UPDATE od_stencil_process set pipe_no=#{pipe_no},operation_time=#{operation_time},operator_no=#{operator_no},stencil_content=#{stencil_content},center_line_color=#{center_line_color},pipe_end_color=#{pipe_end_color},upload_files=#{upload_files},remark=#{remark},result=#{result} where id=#{id}
    </update>


    <select id="getCount" resultType="int">
        select count(*) from od_stencil_process
    </select>

    <resultMap id="all" type="com.htcsweb.entity.OdStencilProcess">
        <id property="id" column="id"></id>
        <result property="pipe_no" column="pipe_no"></result>
        <result property="operation_time" column="operation_time"></result>
        <result property="operator_no" column="operator_no"></result>
        <result property="stencil_content" column="stencil_content"></result>
        <result property="center_line_color" column="center_line_color"></result>
        <result property="pipe_end_color" column="pipe_end_color"></result>
        <result property="upload_files" column="upload_files"></result>
        <result property="remark" column="remark"></result>
        <result property="result" column="result"></result>
    </resultMap>

    <select id="feny"  resultMap="all">
        select * from od_stencil_process where id limit #{arg0},#{arg1}
    </select>

    <select id="getOdStencilProcessById" parameterType="int" resultType="com.htcsweb.entity.OdStencilProcess">
        select * from od_stencil_process where id=#{id}
    </select>
</mapper>